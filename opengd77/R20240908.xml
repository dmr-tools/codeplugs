<?xml version="1.0" encoding="UTF-8"?>
<!--vi: set ts=2 sw=2:-->
<codeplug>
  <meta>
    <name>OpengGD77</name>
    <description>
      This document describes the codeplug format of the OpenGD77 firmware running on a GD77.
    </description>
    <firmware>R202409080</firmware>
  </meta>
  <element at="80h">
    <meta>
      <name>Settings</name>
    </meta>
    <int width="2h" format="bcd" endian="little" default="0">
      <meta>
        <name>UHF min frequency</name>
        <short-name>UHFmin</short-name>
        <description>in MHz.</description>
        <firmware>R202409080</firmware>
      </meta>
    </int>
    <int width="2h" format="bcd" endian="little" default="0">
      <meta>
        <name>UHF max frequency</name>
        <short-name>UHFmax</short-name>
        <description>in MHz.</description>
        <firmware>R202409080</firmware>
      </meta>
    </int>
    <int width="2h" format="bcd" endian="little" default="311">
      <meta>
        <name>VHF min frequency</name>
        <short-name>VHFmin</short-name>
        <description>in MHz.</description>
        <firmware>R202409080</firmware>
      </meta>
    </int>
    <int width="2h" format="bcd" endian="little" default="368">
      <meta>
        <name>VHF max frequency</name>
        <short-name>VHFmax</short-name>
        <description>Maximum frequency for VHF band in MHz.</description>
        <firmware>R202409080</firmware>
      </meta>
    </int>
    <unknown width="58h">
      <meta>
        <name>Unknown data</name>
      </meta>
    </unknown>
    <string format="ascii" width="8" pad="597">
      <meta>
        <name>Callsign</name>
        <short-name>Call</short-name>
        <firmware>R202409080</firmware>
      </meta>
    </string>
    <int width="4h" format="bcd" endian="big" default="0">
      <meta>
        <name>DMR ID</name>
        <firmware>R202409080</firmware>
      </meta>
    </int>
    <unknown width="24h">
      <meta>
        <name>Unknown data</name>
      </meta>
    </unknown>
  </element>
  <repeat at="1588h" n="8">
    <meta>
      <name>APRS Systems</name>
      <firmware>R202409080</firmware>
    </meta>
    <element>
      <meta>
        <name>APRS System</name>
        <firmware>R202409080</firmware>
      </meta>
      <string format="ascii" width="8" pad="597">
        <meta>
          <name>Name</name>
          <firmware>R202409080</firmware>
        </meta>
      </string>
      <int width="1h" format="unsigned">
        <meta>
          <name>Source SSID</name>
          <short-name>S-SSID</short-name>
          <description>SSID of the transmitter.</description>
          <firmware>R202409080</firmware>
        </meta>
      </int>
      <unknown width="3h">
        <meta>
          <name>Latitude</name>
          <firmware>R202409080</firmware>
        </meta>
      </unknown>
      <unknown width="3h">
        <meta>
          <name>Longitude</name>
        </meta>
      </unknown>
      <string format="ascii" width="6" pad="0">
        <meta>
          <name>Via-1 Call</name>
          <short-name>Via1</short-name>
          <firmware>R202409080</firmware>
        </meta>
      </string>
      <int width="1h" format="unsigned" default="0">
        <meta>
          <name>Via-1 SSID</name>
          <firmware>R202409080</firmware>
        </meta>
      </int>
      <string format="ascii" width="6" pad="0">
        <meta>
          <name>Via-2 Call</name>
          <firmware>R202409080</firmware>
        </meta>
      </string>
      <int width="1h" format="unsigned" default="0">
        <meta>
          <name>Via-2 SSID</name>
          <firmware>R202409080</firmware>
        </meta>
      </int>
      <enum width="1h">
        <meta>
          <name>APRS Icon Table</name>
          <short-name>IcnTab</short-name>
          <description>Specifies the APRS icon table.</description>
          <firmware>R202409080</firmware>
        </meta>
        <item value="0">
          <name>Primary</name>
        </item>
        <item value="1">
          <name>Alternate</name>
        </item>
      </enum>
      <int width="1h" format="unsigned">
        <meta>
          <name>APRS Icon index</name>
          <description>Specifies the icon index within the icon table.</description>
          <firmware>R202409080</firmware>
        </meta>
      </int>
      <string format="ascii" width="23" pad="0">
        <meta>
          <name>Comment</name>
          <firmware>R202409080</firmware>
        </meta>
      </string>
      <unknown width="7h">
        <meta>
          <name>Unknown data</name>
        </meta>
      </unknown>
      <enum width="0h:4">
        <meta>
          <name>Position Masking</name>
          <short-name>Mask</short-name>
          <firmware>R202409080</firmware>
        </meta>
        <item value="0">
          <name>None</name>
        </item>
        <item value="1">
          <name>0.0005°</name>
        </item>
        <item value="2">
          <name>0.001°</name>
        </item>
        <item value="3">
          <name>0.005°</name>
        </item>
        <item value="4">
          <name>0.01°</name>
        </item>
        <item value="5">
          <name>0.05°</name>
        </item>
        <item value="6">
          <name>0.1°</name>
        </item>
        <item value="7">
          <name>0.5°</name>
        </item>
      </enum>
      <unused width="0h:2">
        <meta>
          <name>Unused data</name>
          <firmware>R202409080</firmware>
        </meta>
      </unused>
      <int width="0h:1" format="unsigned">
        <meta>
          <name>Use position</name>
          <short-name>POS</short-name>
          <firmware>R202409080</firmware>
        </meta>
      </int>
      <enum width="0h:1">
        <meta>
          <name>Baud rate</name>
          <short-name>BRT</short-name>
          <firmware>R202409080</firmware>
        </meta>
        <item value="0">
          <name>1200 Baud</name>
        </item>
        <item value="1">
          <name>300 Baud</name>
        </item>
      </enum>
      <unknown width="2h">
        <meta>
          <name>Unknown data</name>
        </meta>
      </unknown>
    </element>
  </repeat>
  <repeat at="2f88h" n="64">
    <meta>
      <name>DTMF Contacts</name>
      <description>Bank of 64 DTMF contacts.</description>
      <firmware>R202409080</firmware>
    </meta>
    <element>
      <meta>
        <name>DTMF Contact</name>
        <description>A DTMF contact. Invalid entries have empty names.</description>
        <firmware>R202409080</firmware>
      </meta>
      <string format="ascii" width="16" pad="597">
        <meta>
          <name>Name</name>
          <firmware>R202409080</firmware>
        </meta>
      </string>
      <repeat n="16">
        <meta>
          <name>Number</name>
          <firmware>R202409080</firmware>
        </meta>
        <int width="1h" format="unsigned">
          <meta>
            <name>Digit</name>
            <description>0h-fh Corresponding to 0-9, A, B, C, D, *, #.</description>
            <firmware>R202409080</firmware>
          </meta>
        </int>
      </repeat>
    </element>
  </repeat>
  <unknown at="7500h" width="18h">
    <meta>
      <name>Unknown data</name>
      <firmware>R202409080</firmware>
    </meta>
  </unknown>
  <element at="7518h">
    <meta>
      <name>Boot Settings</name>
      <firmware>R202409080</firmware>
    </meta>
    <enum width="1h">
      <meta>
        <name>Boot display</name>
        <firmware>R202409080</firmware>
      </meta>
      <item value="0">
        <name>Picture</name>
      </item>
      <item value="1">
        <name>Text</name>
      </item>
    </enum>
    <int width="1h" format="unsigned" default="0">
      <meta>
        <name>Boot password enable</name>
        <short-name>PWD</short-name>
        <firmware>R202409080</firmware>
      </meta>
    </int>
    <unknown width="ah">
      <meta>
        <name>Unknown data</name>
      </meta>
    </unknown>
    <int width="3h" format="bcd" endian="big" default="0">
      <meta>
        <name>Boot password</name>
        <short-name>PWD</short-name>
        <firmware>R202409080</firmware>
      </meta>
    </int>
    <unknown width="19h">
      <meta>
        <name>Unknown data</name>
      </meta>
    </unknown>
    <string format="ascii" width="16" pad="597">
      <meta>
        <name>Boot Text Line 1</name>
        <firmware>R202409080</firmware>
      </meta>
    </string>
    <string format="ascii" width="16" pad="597">
      <meta>
        <name>Boot Text Line 2</name>
        <firmware>R202409080</firmware>
      </meta>
    </string>
  </element>
</codeplug>
